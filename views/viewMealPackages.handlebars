<br><br>
<div>
    <h1 style="text-align: center;">{{noData}}</h1>
</div>
<!--Meal Packages-->
<div class="container-fluid padding">
    <div class="row text-center padding topmeal-display-information">
        {{#each data}}
            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <img class="image-fluid topmeal-display" src="/img/{{this.img}}" alt="">
            </div>

            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <b>{{this.title}}</b>
            </div>

            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <span class="money"><b><i>$ {{this.price}}</i></b></span>
            </div>

            <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3">
                <a href="/editPackage?title={{this.title}}" class="btn btn-primary">Edit</a>
            </div>
        {{/each}}
    </div>
</div>